Algoritmo ejercicio_1_segun
	Definir n Como Entero;
	Escribir "Escriba un número entre 1 y 9";
	Leer n;
	Segun n hacer
		1:
			Escribir "A"
		2: 
			Escribir "B"
		3:
			Escribir "C"
		4:
			Escribir "D"
		5:
			Escribir "E"
		6:
			Escribir "F"
		7:
			Escribir "G"
		8:
			Escribir "H"
		9:
			Escribir "I"
		De otro modo:
			Escribir "No vale ese numero"
	FinSegun
//}	
FinAlgoritmo
